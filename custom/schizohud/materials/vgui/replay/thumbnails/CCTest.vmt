"UnlitGeneric"
{
	"$basetexture" "vgui/replay/thumbnails/funnyd1"
	"$translucent" 1
	"$vertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1
	
	$alpha 255
	$ccolor "[0.0 0.0 0.0]"
	$zero 0

	Proxies {

	CrosshairColor
        {
        "resultVar" "$ccolor"
        }
	Clamp
	{
	"srcVar1" "$zero"
	"min" "$ccolor[0]"
	"max" "$ccolor[0]"
	"resultVar" "$frame"
	}
	}
}
